<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listuserOneRowModel" type="com.ruquaiyasapplication.app.modules.interview.data.model.ListuserOneRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowuserOne"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearRowuser"
            android:layout_height="wrap_content"
            android:layout_weight="0.56" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageUserOne"
                android:layout_height="@dimen/_8pxh"
                android:layout_marginBottom="@dimen/_4pxv"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_width="@dimen/_11pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_user_gray_403"
                tools:ignore="ContentDescription" tools:src="@drawable/img_user_gray_403"/>
            <FrameLayout android:id="@+id/frameStackheadline"
                android:layout_height="@dimen/_18pxv"
                android:layout_marginStart="@dimen/_2pxh" android:layout_width="@dimen/_43pxh">
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtHeadlineNineteen"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{listuserOneRowModel.txtHeadlineNineteen}"
                    style="@style/txtPoppinssemibold8" tools:text="@string/lbl_ali_butt"/>
                <TextView android:ellipsize="end"
                    android:gravity="top|start"
                    android:id="@+id/txtIslamabadPkFive"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{listuserOneRowModel.txtIslamabadPkFive}"
                    style="@style/txtPoppinsmedium6" tools:text="@string/lbl_islamabad_pk"/>
            </FrameLayout>
        </LinearLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtHeadlineOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_5pxv"
            android:layout_marginStart="@dimen/_75pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.44" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{listuserOneRowModel.txtHeadlineOne}"
            style="@style/txtPoppinssemibold8" tools:text="@string/lbl_it_manager2"/>
    </LinearLayout>
</layout>
