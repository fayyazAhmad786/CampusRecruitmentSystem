<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".student.presentation.quiz.QuizScreen1">

    <FrameLayout
        android:id="@+id/frameJobDetailsThr"
        style="@style/groupStylegray_50"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearColumnmaskgroup"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="top|start"
            android:layout_marginBottom="@dimen/_10pxv"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/frameStackmaskgroup"
                style="@style/groupStylepurple_900"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_259pxv"
                android:gravity="start">

                <ImageView
                    android:id="@+id/imageMaskGroup"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_259pxh"
                    android:layout_gravity="start"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_maskgroup_green_600"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_maskgroup_green_600" />

                <LinearLayout
                    android:id="@+id/linearRowarrowleft"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageArrowleft"
                        android:layout_width="@dimen/_8pxh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_208pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_white_a700"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowleft_white_a700" />

                    <LinearLayout
                        android:id="@+id/linearColumnfacebook"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_weight="0.92"
                        android:orientation="vertical">

                        <ImageButton
                            android:id="@+id/btnFacebook"
                            android:layout_width="@dimen/_80pxh"
                            android:layout_height="@dimen/_80pxh"
                            android:layout_gravity="center_horizontal"
                            android:layout_marginStart="@dimen/_59pxh"
                            android:layout_marginEnd="@dimen/_59pxh"
                            android:background="@drawable/rectangle_bg_white_a700_radius_40"
                            android:paddingHorizontal="@dimen/_20pxh"
                            android:paddingVertical="@dimen/_20pxv"
                            android:scaleType="fitCenter"
                            android:src="@drawable/img_facebook_deep_purple_300"
                            tools:ignore="ContentDescription" />

                        <TextView
                            android:id="@+id/txtSoftwareEngine"
                            style="@style/txtAbhayaLibreMediumabhayalibremedium20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_59pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_59pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:singleLine="true"
                            android:text="@string/msg_software_engine"
                            tools:text="@string/msg_software_engine" />

                        <TextView
                            android:id="@+id/txtFacebookOne"
                            style="@style/txtAbhayaLibreMediumabhayalibremedium15"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:layout_marginStart="@dimen/_104pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_104pxh"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/lbl_facebook"
                            tools:text="@string/lbl_facebook" />

                        <LinearLayout
                            android:id="@+id/linearRowprice"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_66pxv"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/txtPrice"
                                style="@style/txtPoppinssemibold16_2"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_weight="0.52"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/lbl_180_00_year"
                                tools:text="@string/lbl_180_00_year" />

                            <TextView
                                android:id="@+id/txtSeattleUSA"
                                style="@style/txtPoppinssemibold16_2"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_78pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.48"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/lbl_seattle_usa"
                                tools:text="@string/lbl_seattle_usa" />
                        </LinearLayout>
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/imageBookmark"
                        android:layout_width="@dimen/_17pxh"
                        android:layout_height="@dimen/_19pxh"
                        android:layout_marginStart="@dimen/_6pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_marginBottom="@dimen/_206pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bookmark_white_a700"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_bookmark_white_a700" />
                </LinearLayout>
            </FrameLayout>

            <LinearLayout
                android:id="@+id/linearRowdescription"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_35pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtDescription"
                    style="@style/txtAbhayaLibreMediumabhayalibremedium14"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.3"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_description"
                    tools:text="@string/lbl_description" />

                <TextView
                    android:id="@+id/txtRequirement"
                    style="@style/txtAbhayaLibreMediumabhayalibremedium14_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_35pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.33"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_requirement"
                    tools:text="@string/lbl_requirement" />

                <TextView
                    android:id="@+id/txtAbout"
                    style="@style/txtAbhayaLibreMediumabhayalibremedium14_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_27pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.16"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_about"
                    tools:text="@string/lbl_about" />

                <TextView
                    android:id="@+id/txtReviews"
                    style="@style/txtAbhayaLibreMediumabhayalibremedium14_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.21"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_reviews"
                    tools:text="@string/lbl_reviews" />
            </LinearLayout>

            <TextView
                android:id="@+id/txtDescriptionOne"
                style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:gravity="center_horizontal"
                android:lineSpacingExtra="@dimen/_11pxh"
                android:text="@string/msg_we_are_the_team"
                tools:text="@string/msg_we_are_the_team" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnStartQuiz"
                style="@style/btnSolidRounded_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_start_quiz"
                android:textAllCaps="false"
                tools:text="@string/lbl_start_quiz" />
        </LinearLayout>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>