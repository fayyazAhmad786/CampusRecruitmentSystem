<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="resetPasswordConfirmationVM" type="com.ruquaiyasapplication.app.modules.resetpasswordconfirmation.data.viewmodel.ResetPasswordConfirmationVM"/>
    </data>
    <LinearLayout android:id="@+id/linearResetPassword"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageClose"
            android:layout_gravity="start"
            android:layout_height="@dimen/_16pxh"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="@dimen/_16pxh"
            android:scaleType="fitXY" android:src="@drawable/img_close"
            tools:ignore="ContentDescription" tools:src="@drawable/img_close"/>
        <TextView android:ellipsize="end" android:id="@+id/txtJbizz"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{resetPasswordConfirmationVM.resetPasswordConfirmationModel.txtJbizz}"
            style="@style/txtPoppinssemibold22_1" tools:text="@string/lbl_j_bizz"/>
        <FrameLayout android:id="@+id/frameStackfreepikpath"
            android:layout_gravity="start"
            android:layout_height="@dimen/_239pxv"
            android:layout_marginEnd="@dimen/_71pxh"
            android:layout_marginStart="@dimen/_71pxh"
            android:layout_marginTop="@dimen/_17pxv" android:layout_width="@dimen/_214pxh">
            <ImageView android:id="@+id/imageFreepikpath"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_9pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_21pxh"
                android:layout_marginStart="@dimen/_21pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_170pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_freepikpath"
                tools:ignore="ContentDescription" tools:src="@drawable/img_freepikpath"/>
            <ImageView android:id="@+id/imageFreepikSmartp"
                android:layout_gravity="end"
                android:layout_height="@dimen/_237pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_width="@dimen/_128pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_freepiksmartp"
                tools:ignore="ContentDescription" tools:src="@drawable/img_freepiksmartp"/>
            <ImageView android:id="@+id/imageArrowup"
                android:layout_gravity="bottom|start"
                android:layout_height="@dimen/_214pxh"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_127pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_arrowup"
                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowup"/>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtHeadline"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{resetPasswordConfirmationVM.resetPasswordConfirmationModel.txtHeadline}"
            style="@style/txtPoppinssemibold24" tools:text="@string/lbl_confirmation"/>
        <TextView android:gravity="center_horizontal"
            android:id="@+id/txtHeadlineOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="match_parent"
            android:text="@{resetPasswordConfirmationVM.resetPasswordConfirmationModel.txtHeadlineOne}"
            style="@style/txtPoppinsregular14" tools:text="@string/msg_your_password_h"/>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnLogIn"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_156pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@string/lbl_log_in"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_log_in"/>
    </LinearLayout>
</layout>
