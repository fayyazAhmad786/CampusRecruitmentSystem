<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".student.presentation.quiz.QuizScreen2">

    <FrameLayout
        android:id="@+id/frameJobDetailsThr"
        style="@style/groupStylegray_50"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/linearColumnmaskgroup"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="top|start"
            android:layout_marginBottom="@dimen/_10pxv"
            android:orientation="vertical">

            <FrameLayout
                android:id="@+id/frameStackmaskgroup"
                style="@style/groupStylepurple_900"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_259pxv"
                android:gravity="start">

                <ImageView
                    android:id="@+id/imageMaskGroup"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_259pxh"
                    android:layout_gravity="start"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_maskgroup_green_600"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_maskgroup_green_600" />

                <LinearLayout
                    android:id="@+id/linearRowarrowleft"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginBottom="@dimen/_17pxv"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageArrowleft"
                        android:layout_width="@dimen/_8pxh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_marginBottom="@dimen/_208pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft_white_a700"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowleft_white_a700" />

                    <LinearLayout
                        android:id="@+id/linearColumnfacebook"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_15pxh"
                        android:layout_weight="0.92"
                        android:orientation="vertical">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/img_quiz"
                            android:layout_width="@dimen/_80pxh"
                            android:layout_height="@dimen/_80pxh"
                            android:src="@drawable/img_ellipse"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_gravity="center"
                            android:scaleType="centerCrop"
                            tools:ignore="ContentDescription"
                            app:civ_border_color="#FF000000"/>


                        <TextView
                            android:id="@+id/tv_Job_title_quiz"
                            style="@style/txtAbhayaLibreMediumabhayalibremedium20"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_59pxh"
                            android:layout_marginTop="@dimen/_12pxv"
                            android:layout_marginEnd="@dimen/_59pxh"
                            android:ellipsize="end"
                            android:gravity="center_horizontal"
                            android:singleLine="true"
                            android:text="@string/msg_software_engine"
                            tools:text="@string/msg_software_engine" />

                        <TextView
                            android:id="@+id/tv_company_name_quiz"
                            style="@style/txtAbhayaLibreMediumabhayalibremedium15"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="end"
                            android:layout_marginStart="@dimen/_104pxh"
                            android:layout_marginTop="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_104pxh"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/lbl_facebook"
                            tools:text="@string/lbl_facebook" />

                        <LinearLayout
                            android:id="@+id/linearRowprice"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_56pxv"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/tv_salery_range_quiz"
                                style="@style/txtPoppinssemibold16_2"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_weight="0.52"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/lbl_180_00_year"
                                tools:text="@string/lbl_180_00_year" />

                            <TextView
                                android:id="@+id/tv_job_location_quiz"
                                style="@style/txtPoppinssemibold16_2"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_78pxh"
                                android:layout_marginTop="@dimen/_1pxv"
                                android:layout_weight="0.48"
                                android:ellipsize="end"
                                android:singleLine="true"
                                android:text="@string/lbl_seattle_usa"
                                tools:text="@string/lbl_seattle_usa" />
                        </LinearLayout>
                    </LinearLayout>

                </LinearLayout>
            </FrameLayout>


            <TextView
                android:id="@+id/tv_job_description_quiz"
                style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:gravity="center_horizontal"
                android:visibility="gone"
                android:lineSpacingExtra="@dimen/_11pxh"
                android:text="@string/msg_we_are_the_team"
                tools:text="@string/msg_we_are_the_team" />
            <LinearLayout
                android:id="@+id/linearColumnuploadyourcv"
                style="@style/groupStylecornerRadius_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/txtUploadyourCV"
                    style="@style/txtPoppinsmedium13"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_41pxv"
                    android:layout_marginEnd="@dimen/_32pxh"
                    android:gravity="center_horizontal"
                    android:text="@string/msg_upload_your_cv"
                    tools:text="@string/msg_upload_your_cv" />

                <LinearLayout
                    android:id="@+id/linearColumnuploadADocdocxpdf"
                    style="@style/groupStylecornerRadius_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_32pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_marginEnd="@dimen/_32pxh"
                    android:gravity="center_horizontal"
                    android:orientation="vertical">

                    <androidx.appcompat.widget.AppCompatButton
                        android:id="@+id/btnUploadADocdocxpdf"
                        style="@style/btnSolidRounded_2"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_26pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingBottom="@dimen/_26pxv"
                        android:singleLine="true"
                        android:text="@string/msg_upload_a_doc_do"
                        android:textAllCaps="false"
                        tools:text="@string/msg_upload_a_doc_do" />
                </LinearLayout>

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/btnUpload"
                    style="@style/btnSolidRounded_3"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_64pxh"
                    android:layout_marginTop="@dimen/_32pxv"
                    android:layout_marginEnd="@dimen/_64pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:ellipsize="end"
                    android:onClick="resumeupload"
                    android:gravity="center"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_upload"
                    android:textAllCaps="false"
                    tools:text="@string/lbl_upload" />

                <ImageView
                    android:id="@+id/img_doc_uploaded"
                    android:layout_width="@dimen/_50pxh"
                    android:layout_height="@dimen/_50pxh"
                    android:layout_marginBottom="@dimen/_20pxv"
                    android:src="@drawable/tick"
                    android:visibility="gone"
                    android:layout_gravity="center"/>
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnGetStartedQuiz"
                style="@style/btnSolidRounded_3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:ellipsize="end"
                android:onClick="clickFunction"
                android:gravity="center"
                android:layout_gravity="bottom"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_get_started_quiz"
                android:textAllCaps="false"
                tools:text="@string/lbl_get_started_quiz" />
        </LinearLayout>
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>