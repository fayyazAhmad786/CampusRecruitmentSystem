<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="applySuccessTwoVM" type="com.ruquaiyasapplication.app.modules.applysuccesstwo.data.viewmodel.ApplySuccessTwoVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearApplySuccessT"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:id="@+id/linearColumnarrowleft"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_4">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTypeEight"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_102pxh"
                        android:layout_weight="0.93"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{applySuccessTwoVM.applySuccessTwoModel.txtTypeEight}"
                        style="@style/txtPoppinssemibold16" tools:text="@string/lbl_confirmation"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowspotigy"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <FrameLayout android:id="@+id/frameStackspotigy"
                    android:layout_height="@dimen/_44pxv"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_44pxh" style="@style/groupStylewhite_5">
                    <ImageView android:id="@+id/imageSpotigy"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_44pxh"
                        android:layout_width="@dimen/_44pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_image4"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_image4"/>
                </FrameLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnjrexecutive"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_weight="0.83"
                    android:layout_width="0dp" android:orientation="vertical">
                    <LinearLayout android:id="@+id/linearRowjrexecutive"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtJrExecutive"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.51"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{applySuccessTwoVM.applySuccessTwoModel.txtJrExecutive}"
                            style="@style/txtPoppinssemibold14" tools:text="@string/lbl_ux_intern"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_94pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.49"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{applySuccessTwoVM.applySuccessTwoModel.txtPrice}"
                            style="@style/txtPoppinsmedium12" tools:text="@string/lbl_88_000_y"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearRowburgerking"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtBurgerKing"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_weight="0.33"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{applySuccessTwoVM.applySuccessTwoModel.txtBurgerKing}"
                            style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_spotify"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLosAngelsUS"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_87pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.67"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{applySuccessTwoVM.applySuccessTwoModel.txtLosAngelsUS}"
                            style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_los_angels_us"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnellipse790"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_4">
            <FrameLayout android:id="@+id/frameStackellipse790"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_189pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_27pxv" android:layout_width="@dimen/_254pxh">
                <View
                    android:background="@drawable/rectangle_bg_gray_51_radius_81"
                    android:id="@+id/viewEllipse790"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_162pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_40pxh"
                    android:layout_marginStart="@dimen/_40pxh"
                    android:layout_marginTop="@dimen/_13pxv" android:layout_width="@dimen/_162pxh"/>
                <ImageView android:id="@+id/imageDonerafikiOne"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_189pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_emojioneflagf"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_emojioneflagf"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSuccessful"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{applySuccessTwoVM.applySuccessTwoModel.txtSuccessful}"
                style="@style/txtPoppinsbold24" tools:text="@string/lbl_successful"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtYouvesuccessf"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:text="@{applySuccessTwoVM.applySuccessTwoModel.txtYouvesuccessf}"
                style="@style/txtPoppinsregular16_1" tools:text="@string/msg_you_ve_successf"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnTrackJob"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_84pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_track_job"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_3" tools:text="@string/lbl_track_job"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnBrowseJobs"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_20pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_16pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_16pxv"
                android:singleLine="true"
                android:text="@string/lbl_browse_jobs"
                android:textAllCaps="false"
                style="@style/btnRoundedOutline_1" tools:text="@string/lbl_browse_jobs"/>
        </LinearLayout>
    </LinearLayout>
</layout>
