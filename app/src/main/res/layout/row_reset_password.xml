<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="resetPasswordRowModel" type="com.ruquaiyasapplication.app.modules.resetpassword.data.model.ResetPasswordRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRoweye"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylecornerRadius">
        <LinearLayout android:id="@+id/linearRowuser"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_weight="0.85" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageUser"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="@dimen/_15pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_user"
                tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewPassword"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_weight="0.87" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{resetPasswordRowModel.txtNewPassword}"
                style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_new_password"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageEye"
            android:layout_height="@dimen/_15pxh"
            android:layout_marginBottom="@dimen/_18pxv"
            android:layout_marginEnd="@dimen/_23pxh"
            android:layout_marginStart="@dimen/_124pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_width="@dimen/_22pxh"
            android:scaleType="fitXY" android:src="@drawable/img_eye"
            tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
    </LinearLayout>
</layout>
