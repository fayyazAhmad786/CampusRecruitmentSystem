<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="logInVM" type="com.ruquaiyasapplication.app.modules.login.data.viewmodel.LogInVM"/>
    </data>
    <LinearLayout android:gravity="start" android:id="@+id/linearLogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ImageView android:id="@+id/imageArrowleft"
            android:layout_height="@dimen/_16pxh"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="@dimen/_8pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowleft"
            tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
        <TextView android:ellipsize="end" android:id="@+id/txtHIREE"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_30pxh"
            android:layout_marginStart="@dimen/_30pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{logInVM.logInModel.txtHIREE}"
            style="@style/txtPoppinssemibold22_2" tools:text="@string/lbl_hiree"/>
        <TextView android:ellipsize="end" android:id="@+id/txtHeadline"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_4pxv"
            android:layout_width="match_parent"
            android:singleLine="true"
            android:text="@{logInVM.logInModel.txtHeadline}"
            style="@style/txtPoppinssemibold24" tools:text="@string/lbl_welcome_back"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtHeadlineOne"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{logInVM.logInModel.txtHeadlineOne}"
            style="@style/txtPoppinsregular14" tools:text="@string/msg_let_s_log_in_a"/>
        <EditText
            android:background="@drawable/rectangle_border_gray_400_radius_10"
            android:drawablePadding="@dimen/_13pxh"
            android:drawableStart="@drawable/img_arrowdown_gray_400_12x15"
            android:ellipsize="end" android:hint="@string/lbl_e_mail"
            android:id="@+id/etGroupFortyEight"
            android:inputType="textEmailAddress"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_49pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_15pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_25pxh"
            android:paddingTop="@dimen/_15pxv" android:singleLine="true"
            android:text="@={logInVM.logInModel.etGroupFortyEightValue}"
            android:textColorHint="@color/gray_400"
            style="@style/etRoundedOutline_1" tools:ignore="Autofill"/>
        <LinearLayout android:id="@+id/linearRoweye"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius">
            <LinearLayout android:id="@+id/linearRowuser"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginStart="@dimen/_26pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.81" android:layout_width="0dp" android:orientation="horizontal">
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_13pxh"
                    android:layout_weight="0.81"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{logInVM.logInModel.txtPassword}"
                    style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_password"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageEye"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginBottom="@dimen/_18pxv"
                android:layout_marginEnd="@dimen/_23pxh"
                android:layout_marginStart="@dimen/_158pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="@dimen/_22pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_eye"
                tools:ignore="ContentDescription" tools:src="@drawable/img_eye"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnLogIn"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@string/lbl_log_in"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_log_in"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtForgotPassword"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_40pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{logInVM.logInModel.txtForgotPassword}"
            style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold15" tools:text="@string/msg_forgot_password"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtHaventanacco"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_216pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{logInVM.logInModel.txtHaventanacco}"
            style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold14_2" tools:text="@string/msg_haven_t_an_acco"/>
    </LinearLayout>
</layout>
