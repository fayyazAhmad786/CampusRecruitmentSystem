<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="jobTypeOneRowModel" type="com.ruquaiyasapplication.app.modules.jobtypeone.data.model.JobTypeOneRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearType"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_2">
        <LinearLayout android:id="@+id/linearRowcurveone"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginStart="@dimen/_32pxh"
            android:layout_marginTop="@dimen/_13pxv"
            android:layout_weight="0.84" android:layout_width="0dp" android:orientation="horizontal">
            <ImageView android:id="@+id/imageCurveOne"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_curve1_5"
                tools:ignore="ContentDescription" tools:src="@drawable/img_curve1_5"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtTypeTen"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_weight="0.78" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{jobTypeOneRowModel.txtTypeTen}"
                style="@style/txtPoppinsregular16" tools:text="@string/lbl_full_time"/>
        </LinearLayout>
        <View
            android:background="@drawable/rectangle_border_deep_purple_300_radius_10"
            android:id="@+id/viewEllipse773"
            android:layout_height="@dimen/_20pxh"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_31pxh"
            android:layout_marginStart="@dimen/_137pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_20pxh"/>
    </LinearLayout>
</layout>
