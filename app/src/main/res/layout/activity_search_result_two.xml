<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="searchResultTwoVM"
            type="com.ruquaiyasapplication.app.modules.searchresulttwo.data.viewmodel.SearchResultTwoVM" />
    </data>

    <LinearLayout
        android:id="@+id/linearSearchResultT"
        style="@style/groupStylegray_50"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/linearColumnarrowleft"
            style="@style/groupStylewhite_A700"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:gravity="center_horizontal"
                app:contentInsetStart="0dp">

                <LinearLayout
                    android:id="@+id/linearRowarrowright"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/linearRowarrowleft"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="0.92"
                        android:orientation="horizontal">

                        <ImageView
                            android:id="@+id/imageArrowleft"
                            android:layout_width="@dimen/_8pxh"
                            android:layout_height="@dimen/_16pxh"
                            android:layout_marginTop="@dimen/_3pxv"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_arrowleft"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_arrowleft" />

                        <TextView
                            android:id="@+id/txtTypeEight"
                            style="@style/txtPoppinssemibold16"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_26pxh"
                            android:layout_weight="0.92"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@{searchResultTwoVM.searchResultTwoModel.txtTypeEight}"
                            tools:text="@string/lbl_ux_designer" />
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/imageArrowright"
                        android:layout_width="@dimen/_12pxh"
                        android:layout_height="@dimen/_12pxh"
                        android:layout_marginStart="@dimen/_183pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowright_gray_400"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_arrowright_gray_400" />
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>

            <View
                android:id="@+id/lineRectangleNinety"
                android:layout_width="@dimen/_327pxh"
                android:layout_height="@dimen/_1pxv"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_9pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:background="@color/gray_401" />

            <LinearLayout
                android:id="@+id/linearRow293jobsfound"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_24pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_marginEnd="@dimen/_24pxh"
                android:layout_marginBottom="@dimen/_16pxv"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt293JobsFound"
                    style="@style/txtPoppinsmedium14_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.9"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{searchResultTwoVM.searchResultTwoModel.txt293JobsFound}"
                    tools:text="@string/lbl_293_jobs_found" />

                <ImageView
                    android:id="@+id/imageThumbsup"
                    android:layout_width="@dimen/_12pxh"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_204pxh"
                    android:layout_marginTop="@dimen/_4pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_thumbsup_indigo_600"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_thumbsup_indigo_600" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearRowrecomendedjobs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_18pxv"
            android:layout_marginEnd="@dimen/_26pxh"
            android:gravity="start"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtRecomendedJobs"
                style="@style/txtPoppinssemibold16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.8"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{searchResultTwoVM.searchResultTwoModel.txtRecomendedJobs}"
                tools:text="@string/msg_recommended_job" />

            <TextView
                android:id="@+id/txtSeeall"
                style="@style/txtPoppinsregular13"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_99pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.2"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{searchResultTwoVM.searchResultTwoModel.txtSeeall}"
                tools:text="@string/lbl_see_all" />
        </LinearLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerListlogosdribbble"
            itemSpace="@{@dimen/_15pxh}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:gravity="center_horizontal"
            android:orientation="vertical"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            tools:itemCount="2"
            tools:listitem="@layout/row_listlogosdribbble" />

        <LinearLayout
            android:id="@+id/linearRowpopularjobs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_26pxh"
            android:layout_marginTop="@dimen/_24pxv"
            android:layout_marginEnd="@dimen/_26pxh"
            android:gravity="start"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtPopularJobs"
                style="@style/txtPoppinssemibold16"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.71"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{searchResultTwoVM.searchResultTwoModel.txtPopularJobs}"
                tools:text="@string/lbl_popular_jobs" />

            <TextView
                android:id="@+id/txtSeeallOne"
                style="@style/txtPoppinsregular13"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_160pxh"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.29"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{searchResultTwoVM.searchResultTwoModel.txtSeeallOne}"
                tools:text="@string/lbl_see_all" />
        </LinearLayout>

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerListuxdesignerTwo"
            itemSpace="@{@dimen/_15pxh}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_12pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
            tools:itemCount="2"
            tools:listitem="@layout/row_listuxdesigner_two" />
    </LinearLayout>
</layout>
