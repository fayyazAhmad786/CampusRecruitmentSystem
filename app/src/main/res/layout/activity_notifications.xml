<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationsVM" type="com.ruquaiyasapplication.app.modules.notifications.data.viewmodel.NotificationsVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_10pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearTop"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_50">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowleft"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowleft"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginBottom="@dimen/_2pxv"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_8pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowleft"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTypeEight"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_104pxh"
                        android:layout_weight="0.93"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{notificationsVM.notificationsModel.txtTypeEight}"
                        style="@style/txtPoppinssemibold16" tools:text="@string/lbl_notifications"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearNotifications"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_50">
            <TextView android:ellipsize="end"
                android:id="@+id/txtNewactivity"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{notificationsVM.notificationsModel.txtNewactivity}"
                style="@style/txtPoppinssemibold14" tools:text="@string/lbl_new_activity"/>
            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListgooglewantto"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_13pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_16pxh}" tools:itemCount="2" tools:listitem="@layout/row_listgooglewantto"/>
            <FrameLayout android:id="@+id/frameStackapplications"
                android:layout_height="@dimen/_408pxv"
                android:layout_marginTop="@dimen/_31pxv" android:layout_width="@dimen/_327pxh">
                <LinearLayout android:id="@+id/linearColumnapplications"
                    android:layout_gravity="top|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/linearRowapplications"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtApplications"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.67"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{notificationsVM.notificationsModel.txtApplications}"
                            style="@style/txtPoppinssemibold14" tools:text="@string/lbl_applications"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtSeeall"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_195pxh"
                            android:layout_weight="0.33"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{notificationsVM.notificationsModel.txtSeeall}"
                            style="@style/txtPoppinsmedium13_2" tools:text="@string/lbl_see_all"/>
                    </LinearLayout>
                    <LinearLayout android:id="@+id/linearNotification"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <FrameLayout android:id="@+id/frameStack"
                            android:layout_height="@dimen/_40pxv"
                            android:layout_marginBottom="@dimen/_26pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_40pxh" style="@style/groupStylegray_401cornerRadius">
                            <ImageView
                                android:id="@+id/imageImageSeventyNine"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_40pxh"
                                android:layout_width="@dimen/_40pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_image79"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_image79"/>
                        </FrameLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumngooglewanttoOne"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_6pxh"
                            android:layout_marginStart="@dimen/_12pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_weight="0.87"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView
                                android:id="@+id/txtGooglewanttoOne"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:text="@{notificationsVM.notificationsModel.txtGooglewanttoOne}"
                                style="@style/txtPoppinsmedium15_2" tools:text="@string/msg_your_applicatio"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txt4hrsago"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_2pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{notificationsVM.notificationsModel.txt4hrsago}"
                                style="@style/txtPoppinsmedium13_3" tools:text="@string/lbl_1_hrs_ago"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearNotification1"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <FrameLayout
                            android:id="@+id/frameStackellipse782"
                            android:layout_height="@dimen/_40pxv"
                            android:layout_marginBottom="@dimen/_26pxv"
                            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_40pxh">
                            <View
                                android:background="@drawable/rectangle_bg_gray_401_radius_20"
                                android:id="@+id/viewEllipse782"
                                android:layout_gravity="center"
                                android:layout_height="@dimen/_40pxh"
                                android:layout_marginEnd="@dimen/_1pxh" android:layout_width="@dimen/_40pxh"/>
                            <ImageView
                                android:id="@+id/imageImageNinetySix"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_40pxh"
                                android:layout_width="@dimen/_40pxh"
                                android:scaleType="centerCrop"
                                circular="@{true}"
                                imageRes="@{@drawable/img_image96}"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_image96"/>
                        </FrameLayout>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumngooglewanttoTwo"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_17pxh"
                            android:layout_marginStart="@dimen/_11pxh"
                            android:layout_weight="0.86"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView
                                android:id="@+id/txtGooglewanttoTwo"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:text="@{notificationsVM.notificationsModel.txtGooglewanttoTwo}"
                                style="@style/txtPoppinsmedium15_2" tools:text="@string/msg_booking_com_rev"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txt4hrsagoOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_4pxv"
                                android:layout_width="wrap_content"
                                android:singleLine="true"
                                android:text="@{notificationsVM.notificationsModel.txt4hrsagoOne}"
                                style="@style/txtPoppinsmedium13_3" tools:text="@string/lbl_3_hrs_ago"/>
                        </LinearLayout>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSeeallOne"
                        android:layout_gravity="end"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_31pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{notificationsVM.notificationsModel.txtSeeallOne}"
                        style="@style/txtPoppinsmedium13_2" tools:text="@string/lbl_see_all"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearColumninterview"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtInterview"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{notificationsVM.notificationsModel.txtInterview}"
                        style="@style/txtPoppinssemibold14" tools:text="@string/lbl_interview"/>
                    <androidx.recyclerview.widget.RecyclerView
                        android:id="@+id/recyclerListgooglewanttoThree"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="match_parent"
                        android:orientation="vertical"
                        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                        itemSpace="@{@dimen/_17pxh}" tools:itemCount="2" tools:listitem="@layout/row_listgooglewantto_three"/>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameBottombar"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" style="@style/groupStylewhite_A700">
            <LinearLayout android:id="@+id/linearRowhome"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_28pxv"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageHome"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_20pxh"
                    android:layout_marginBottom="@dimen/_5pxv"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_home_black_900"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_home_black_900"/>
                <LinearLayout android:id="@+id/linearRowbookmark"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.92"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageBookmark"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_23pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_17pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_bookmark"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_bookmark"/>
                    <ImageView android:id="@+id/imageCompany"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_50pxh"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_company"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_company"/>
                    <ImageView android:id="@+id/imageNotification"
                        android:layout_gravity="center"
                        android:layout_height="@dimen/_25pxh"
                        android:layout_marginEnd="@dimen/_78pxh"
                        android:layout_marginStart="@dimen/_47pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_25pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_notification"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_notification"/>
                </LinearLayout>
            </LinearLayout>
        </FrameLayout>
    </LinearLayout>
</layout>
