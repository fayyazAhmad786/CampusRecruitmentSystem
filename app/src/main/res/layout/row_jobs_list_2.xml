<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_margin="7dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:id="@+id/linearRowthumbsup"
        style="@style/groupStyledeep_purple_200_68cornerRadius"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/img_company_2"
            android:layout_width="@dimen/_41pxh"
            android:layout_height="@dimen/_43pxh"
            android:src="@drawable/img_ellipse"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_marginBottom="@dimen/_15pxv"
            tools:ignore="ContentDescription"
            app:civ_border_color="#FF000000"/>


        <LinearLayout
            android:id="@+id/linearColumnjrexecutiveOne"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_weight="0.84"
            android:gravity="start"
            android:orientation="vertical">

            <LinearLayout
                android:id="@+id/linearRowjrexecutive"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_Job_title_2"
                    style="@style/txtPoppinssemibold14"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.58"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{listthumbsupOneRowModel.txtJrExecutiveOne}"
                    tools:text="@string/lbl_jr_executive" />

                <TextView
                    android:id="@+id/tv_salery_range_2"
                    style="@style/txtPoppinsmedium12"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_74pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.42"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_96_000_y"
                    tools:text="@string/lbl_96_000_y" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowburgerking"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_company_name_2"
                    style="@style/txtPoppinsregular13_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.45"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_burger_king"
                    tools:text="@string/lbl_burger_king" />

                <TextView
                    android:id="@+id/tv_location_2"
                    style="@style/txtPoppinsregular13_1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.45"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_los_angels_us"
                    tools:text="@string/lbl_los_angels_us" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>

</LinearLayout>