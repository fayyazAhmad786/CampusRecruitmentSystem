<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="appliedRowModel" type="com.ruquaiyasapplication.app.modules.applied.data.model.AppliedRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRow"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStylewhite_A700cornerRadius">
        <ImageView android:id="@+id/imageImageFour"
            android:layout_height="@dimen/_44pxh"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="@dimen/_43pxh"
            android:scaleType="fitXY" android:src="@drawable/img_image4"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image4"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnproductmanager"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_weight="0.84" android:layout_width="0dp" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowproductmanager"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtProductManager"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.46"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{appliedRowModel.txtProductManager}"
                    style="@style/txtPoppinssemibold14" tools:text="@string/lbl_ux_intern"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txt84000y"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_81pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.54"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{appliedRowModel.txt84000y}"
                    style="@style/txtPoppinsmedium12_1" tools:text="@string/lbl_san_jose_us"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowbeats"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_2pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBeats"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.5"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{appliedRowModel.txtBeats}"
                    style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_spotify"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLanguage"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_132pxh"
                    android:layout_weight="0.5"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{appliedRowModel.txtLanguage}"
                    style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_dec_20"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
