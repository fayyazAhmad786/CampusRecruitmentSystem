<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listthumbsupOneRowModel" type="com.ruquaiyasapplication.app.modules.homepagefive.data.model.ListthumbsupOneRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowthumbsup"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:orientation="horizontal" style="@style/groupStyledeep_purple_200_68cornerRadius">
        <ImageView android:id="@+id/imageThumbsupOne"
            android:layout_height="@dimen/_43pxh"
            android:layout_marginBottom="@dimen/_15pxv"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="@dimen/_41pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_thumbsup_light_blue_800"
            tools:ignore="ContentDescription" tools:src="@drawable/img_thumbsup_light_blue_800"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearColumnjrexecutiveOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_14pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_14pxv"
            android:layout_weight="0.84" android:layout_width="0dp" android:orientation="vertical">
            <LinearLayout android:id="@+id/linearRowjrexecutive"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtJrExecutiveOne"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.58"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{listthumbsupOneRowModel.txtJrExecutiveOne}"
                    style="@style/txtPoppinssemibold14" tools:text="@string/lbl_jr_executive"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_74pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.42"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{listthumbsupOneRowModel.txtPriceTwo}"
                    style="@style/txtPoppinsmedium12" tools:text="@string/lbl_96_000_y"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearRowburgerking"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBurgerKing"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_weight="0.45"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{listthumbsupOneRowModel.txtBurgerKing}"
                    style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_burger_king"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtLosAngelsUS"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_58pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.55"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@{listthumbsupOneRowModel.txtLosAngelsUS}"
                    style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_los_angels_us"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
