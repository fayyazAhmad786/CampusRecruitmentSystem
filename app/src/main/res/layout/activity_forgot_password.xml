            android:id="@+id/linearRowthumbsup"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylecornerRadius">
            <FrameLayout android:id="@+id/frameStackthumbsup"
                android:layout_height="@dimen/_12pxv"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_19pxv" android:layout_width="@dimen/_15pxh">
                <ImageView android:id="@+id/imageThumbsup"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_width="@dimen/_15pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_thumbsup"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_thumbsup"/>
                <ImageView android:id="@+id/imageArrowdownOne"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_4pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_2pxh"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_width="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowdown_gray_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown_gray_400"/>
            </FrameLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtEmailOne"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_225pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_weight="0.75" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{forgotPasswordVM.forgotPasswordModel.txtEmailOne}"
                style="@style/txtPoppinsmedium14_1" tools:text="@string/lbl_e_mail"/>
        </LinearLayout>
        <androidx.appcompat.widget.AppCompatButton
            android:ellipsize="end" android:gravity="center"
            android:id="@+id/btnSendCode"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginTop="@dimen/_92pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_16pxv"
            android:paddingEnd="@dimen/_30pxh"
            android:paddingStart="@dimen/_30pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:text="@string/lbl_send_code"
            android:textAllCaps="false" style="@style/btnSolidRounded_1" tools:text="@string/lbl_send_code"/>
    </LinearLayout>
</layout>
