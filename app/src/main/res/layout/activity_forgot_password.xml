                tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown_gray_900"/>
            <View android:background="@color/gray_300"
                android:id="@+id/lineRectangleFortyTwo"
                android:layout_height="@dimen/_23pxv"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.01" android:layout_width="0dp"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txt14842989335"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_16pxv"
                android:layout_marginEnd="@dimen/_123pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_weight="0.8" android:layout_width="0dp"
