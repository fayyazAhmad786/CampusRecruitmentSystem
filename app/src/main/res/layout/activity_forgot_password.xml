                android:text="@{forgotPasswordVM.forgotPasswordModel.txtEmail}"
                style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold13" tools:text="@string/lbl_e_mail"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnMobileNumber"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_6pxv"
                android:layout_marginEnd="@dimen/_6pxh"
                android:layout_marginStart="@dimen/_49pxh"
                android:layout_marginTop="@dimen/_6pxv"
                android:layout_weight="0.79" android:layout_width="0dp"
                android:paddingBottom="@dimen/_10pxv"
                android:paddingEnd="@dimen/_25pxh"
                android:paddingStart="@dimen/_26pxh"
                android:paddingTop="@dimen/_10pxv"
                android:singleLine="true"
                android:text="@string/lbl_mobile_number"
                android:textAllCaps="false"
