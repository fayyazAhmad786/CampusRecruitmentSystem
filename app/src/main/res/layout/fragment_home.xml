<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".student.presentation.fragments.HomeFragment">

    <FrameLayout
        android:id="@+id/frameHomepageFive"
        style="@style/groupStylegray_50"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_27pxv">

        <LinearLayout
            android:id="@+id/linearColumnellipse"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="top|center"
            android:layout_marginStart="@dimen/_24pxh"
            android:layout_marginEnd="@dimen/_24pxh"
            android:layout_marginBottom="@dimen/_10pxv"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:contentInsetStart="0dp">

                <LinearLayout
                    android:id="@+id/linearRowheadline"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:id="@+id/linearColumnheadline"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_weight="0.83"
                        android:gravity="start"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtHeadline"
                            style="@style/txtPoppinsmedium14"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/msg_welcome_to_jobs"
                            tools:text="@string/msg_welcome_to_jobs" />

                        <TextView
                            android:id="@+id/txtHeadlineOne"
                            style="@style/txtPoppinsbold22"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:ellipsize="end"
                            android:singleLine="true"
                            android:text="@string/msg_discover_jobs"
                            tools:text="@string/msg_discover_jobs" />
                    </LinearLayout>

                    <FrameLayout
                        android:id="@+id/frameStackellipse"
                        android:layout_width="@dimen/_54pxh"
                        android:layout_height="@dimen/_54pxv"
                        android:layout_marginTop="@dimen/_1pxv">

                        <ImageView
                            android:id="@+id/imageEllipse"
                            android:src="@drawable/img_ellipse"
                            android:layout_width="@dimen/_54pxh"
                            android:layout_height="@dimen/_54pxh"
                            android:layout_gravity="start"
                            android:scaleType="centerCrop"
                            tools:ignore="ContentDescription"
                            tools:src="@drawable/img_ellipse" />

                        <LinearLayout
                            android:id="@+id/linearColumnellipse767"
                            style="@style/groupStylegray_50cornerRadius"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="top|end"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_4pxv"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:orientation="vertical">

                            <View
                                android:id="@+id/viewEllipse767"
                                android:layout_width="@dimen/_8pxh"
                                android:layout_height="@dimen/_8pxh"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginHorizontal="@dimen/_4pxh"
                                android:layout_marginVertical="@dimen/_4pxv"
                                android:background="@drawable/rectangle_bg_red_a700_radius_4" />
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>

            <LinearLayout
                android:id="@+id/linearRowthumbsup"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_37pxv"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/linearRowrewind"
                    style="@style/groupStylebluegray_50cornerRadius"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.85"
                    android:orientation="horizontal">

                    <ImageView
                        android:id="@+id/imageRewind"
                        android:layout_width="@dimen/_15pxh"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_marginBottom="@dimen/_15pxv"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_rewind"
                        tools:ignore="ContentDescription"
                        tools:src="@drawable/img_rewind" />

                    <TextView
                        android:id="@+id/txtSearchajobor"
                        style="@style/txtPoppinsregular15"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_35pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_weight="0.92"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/msg_search_a_job_or"
                        tools:text="@string/msg_search_a_job_or" />
                </LinearLayout>

                <ImageButton
                    android:id="@+id/btnThumbsup"
                    android:layout_width="@dimen/_48pxh"
                    android:layout_height="@dimen/_48pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:background="@drawable/rectangle_bg_bluegray_50_radius_12"
                    android:paddingHorizontal="@dimen/_15pxh"
                    android:paddingVertical="@dimen/_15pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_thumbsup_indigo_600"
                    tools:ignore="ContentDescription" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearRowfeaturedjobs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_marginEnd="@dimen/_4pxh"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtFeaturedJobs"
                    style="@style/txtPoppinssemibold16"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.75"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_featured_jobs"
                    tools:text="@string/lbl_featured_jobs" />

                <TextView
                    android:id="@+id/txtSeeall"
                    style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold14"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_171pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:layout_weight="0.25"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_see_all"
                    tools:text="@string/lbl_see_all" />
            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListjrexecutive"
                itemSpace="@dimen/_15pxh"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_18pxv"
                android:orientation="horizontal"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:itemCount="2"
                tools:listitem="@layout/row_listjrexecutive" />

            <LinearLayout
                android:id="@+id/linearRowpopularjobs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_40pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:gravity="start"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txtPopularJobs"
                    style="@style/txtPoppinssemibold16"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.71"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_popular_jobs"
                    tools:text="@string/lbl_popular_jobs" />

                <TextView
                    android:id="@+id/txtSeeallOne"
                    style="@style/txtPoppinsregular13"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_160pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginBottom="@dimen/_3pxv"
                    android:layout_weight="0.29"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@string/lbl_see_all"
                    tools:text="@string/lbl_see_all" />
            </LinearLayout>

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/recyclerListthumbsupOne"
                itemSpace="@{@dimen/_17pxh}"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_16pxv"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                tools:itemCount="2"
                tools:listitem="@layout/row_listthumbsup_one" />
        </LinearLayout>
    </FrameLayout>
</FrameLayout>