<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="signUpOneVM" type="com.ruquaiyasapplication.app.modules.signupone.data.viewmodel.SignUpOneVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearColumnarrowleft"
        android:layout_height="match_parent"
        android:layout_marginEnd="@dimen/_38pxh"
        android:layout_marginStart="@dimen/_24pxh"
        android:layout_marginTop="@dimen/_20pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_50">
        <ScrollView android:clipToPadding="false"
            android:fillViewport="true" android:gravity="start"
            android:id="@+id/scrollViewScrollview"
            android:layout_height="0dp" android:layout_weight="1" android:layout_width="match_parent">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSignupOne"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="vertical">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_width="@dimen/_8pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHIREE"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signUpOneVM.signUpOneModel.txtHIREE}"
                    style="@style/txtPoppinssemibold22_2" tools:text="@string/lbl_hiree"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHeadline"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signUpOneVM.signUpOneModel.txtHeadline}"
                    style="@style/txtPoppinssemibold24" tools:text="@string/lbl_sign_up"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtHeadlineOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{signUpOneVM.signUpOneModel.txtHeadlineOne}"
                    style="@style/txtPoppinsregular14" tools:text="@string/msg_let_s_register"/>
                <LinearLayout android:id="@+id/linearRowstudent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_29pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylegray_100cornerRadius">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnStudent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_7pxh"
                        android:layout_marginTop="@dimen/_6pxv"
                        android:layout_weight="0.71"
                        android:layout_width="0dp"
                        android:paddingBottom="@dimen/_10pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_10pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_student"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded" tools:text="@string/lbl_student"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtFacilitator"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_17pxv"
                        android:layout_marginEnd="@dimen/_38pxh"
                        android:layout_marginStart="@dimen/_45pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_weight="0.29"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{signUpOneVM.signUpOneModel.txtFacilitator}"
                        style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold13" tools:text="@string/lbl_facilitator"/>
                </LinearLayout>
                <FrameLayout android:id="@+id/frameStacklock"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="@dimen/_52pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_22pxv" android:layout_width="@dimen/_327pxh">
                    <ImageView android:id="@+id/imageLock"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_18pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_22pxh"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_11pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnFullName"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_15pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_15pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_full_name"
                        android:textAllCaps="false"
                        style="@style/btnRoundedOutline_2" tools:text="@string/lbl_full_name"/>
                </FrameLayout>
                <EditText
                    android:background="@drawable/rectangle_border_gray_400_radius_10"
                    android:drawablePadding="@dimen/_16pxh"
                    android:drawableStart="@drawable/img_arrowdown_gray_400_12x15"
                    android:ellipsize="end"
                    android:hint="@string/lbl_e_mail"
                    android:id="@+id/etGroupFortyEight"
                    android:inputType="textEmailAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_15pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_15pxv"
                    android:singleLine="true"
                    android:text="@={signUpOneVM.signUpOneModel.etGroupFortyEightValue}"
                    android:textColorHint="@color/gray_400"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_border_gray_400_radius_10"
                    android:drawableEnd="@drawable/img_eye_gray_300"
                    android:drawablePadding="@dimen/_30pxh"
                    android:drawableStart="@drawable/img_user"
                    android:ellipsize="end"
                    android:hint="@string/lbl_password"
                    android:id="@+id/etGroupFortySeven"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@={signUpOneVM.signUpOneModel.etGroupFortySevenValue}"
                    android:textColorHint="@color/gray_400"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <EditText
                    android:background="@drawable/rectangle_border_gray_400_radius_10"
                    android:drawableEnd="@drawable/img_eye_gray_300"
                    android:drawablePadding="@dimen/_30pxh"
                    android:drawableStart="@drawable/img_user"
                    android:ellipsize="end"
                    android:hint="@string/msg_confirm_passwor"
                    android:id="@+id/etGroupFiftyEight"
                    android:inputType="textPassword"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_24pxh"
                    android:paddingStart="@dimen/_22pxh"
                    android:paddingTop="@dimen/_14pxv"
                    android:singleLine="true"
                    android:text="@={signUpOneVM.signUpOneModel.etGroupFiftyEightValue}"
                    android:textColorHint="@color/gray_400"
                    style="@style/etRoundedOutline" tools:ignore="Autofill"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowcompany"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius">
                    <ImageView android:id="@+id/imageCompany"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_company_20x20"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_company_20x20"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtEnteryourcomp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_87pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_weight="0.9"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{signUpOneVM.signUpOneModel.txtEnteryourcomp}"
                        style="@style/txtPoppinsmedium14_3" tools:text="@string/msg_enter_your_comp"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearRowarrowdownOne"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius">
                    <LinearLayout android:id="@+id/linearRowlocation"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_18pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_12pxv"
                        android:layout_weight="0.85"
                        android:layout_width="0dp" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageLocation"
                            android:layout_height="@dimen/_20pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_20pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_location"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtLocationOne"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_13pxh"
                            android:layout_weight="0.75"
                            android:layout_width="0dp"
                            android:singleLine="true"
                            android:text="@{signUpOneVM.signUpOneModel.txtLocationOne}"
                            style="@style/txtPoppinsmedium14_3" tools:text="@string/lbl_location"/>
                    </LinearLayout>
                    <ImageView android:id="@+id/imageArrowdownOne"
                        android:layout_height="@dimen/_7pxh"
                        android:layout_marginBottom="@dimen/_22pxv"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_169pxh"
                        android:layout_marginTop="@dimen/_23pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowdown"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowdown"/>
                </LinearLayout>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowmanager"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_16pxv"
                    android:layout_width="match_parent"
                    android:orientation="horizontal" style="@style/groupStylecornerRadius">
                    <ImageView android:id="@+id/imageManager"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginStart="@dimen/_22pxh"
                        android:layout_marginTop="@dimen/_16pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_manager"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_manager"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtNumberofEmplo"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_16pxv"
                        android:layout_marginEnd="@dimen/_126pxh"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_weight="0.88"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@{signUpOneVM.signUpOneModel.txtNumberofEmplo}"
                        style="@style/txtPoppinsmedium14_3" tools:text="@string/msg_number_of_emplo"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnRegister"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_16pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_16pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_register"
                    android:textAllCaps="false"
                    style="@style/btnSolidRounded_1" tools:text="@string/lbl_register"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtConfirmation"
                    android:layout_gravity="center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_23pxh"
                    android:layout_marginStart="@dimen/_23pxh"
                    android:layout_marginTop="@dimen/_38pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@{signUpOneVM.signUpOneModel.txtConfirmation}"
                    style="@style/txtAbhayaLibreSemiBoldabhayalibresemibold14_2" tools:text="@string/msg_have_an_account"/>
            </LinearLayout>
        </ScrollView>
    </LinearLayout>
</layout>
